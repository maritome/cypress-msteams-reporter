{
  "name": "cypress-msteams-reporter",
  "version": "1.0.3",
  "description": "A microsoft teams reporter for allure reports generated by cypress",
  "main": "src/index.js",
  "bin": {
    "cypress-msteams-reporter": "src/index.js"
  },
  "scripts": {
    "generate:allure-report": "allure generate --clean",
    "ms-teams:report": "npx node src/index.js",
    "cypress:tests": "cypress run --env allure=true --spec cypress/integration/msTeamsReporterTests/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maritome/cypress-msteams-reporter.git"
  },
  "author": "Maria Tome",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maritome/cypress-msteams-reporter/issues"
  },
  "homepage": "https://github.com/maritome/cypress-msteams-reporter#readme",
  "dependencies": {
    "commander": "^6.2.1",
    "dotenv": "^8.2.0",
    "ms-teams-webhook": "^2.0.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.0.0",
    "cypress": "^6.2.1"
  }
}
